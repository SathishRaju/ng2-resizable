{
  "name": "ng2-resizable",
  "version": "0.1.1",
  "description": "Angular 2 component for resizable containers",
  "author": "Marc Weinberger",
  "license": "MIT",
  "repository": "https://github.com/mixomat/ng2-resizable",
  "main": "ng2-resizable.js",
  "typings": "ng2-resizable.d.ts",
  "scripts": {
    "clean": "rimraf lib/",
    "sass": "node-sass -q --output-style expanded src/resizable.scss lib/resizable.css",
    "prebuild": "npm run clean && npm run sass",
    "build": "tsc -p src"
